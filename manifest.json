{
  "manifest_version": 3,
  "name": "Auto Fill Quiz",
  "version": "2.0",
  "description": "Tự động điền đáp án trắc nghiệm - Hỗ trợ iframe",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
